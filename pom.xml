                <version>${felles.version}</version>
            </dependency>

            <!-- Reaktor -->
            <dependency>
                <groupId>no.nav.foreldrepenger.fordel</groupId>
                <artifactId>domene</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>no.nav.foreldrepenger.fordel</groupId>
