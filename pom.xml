        <changelist>-SNAPSHOT</changelist>

        <felles.version>2.1.0-20200911114222-6363464</felles.version>
        <prosesstask.version>2.4.3-20200821080802-12519de</prosesstask.version>

        <java.version>14</java.version>
        <argLine>-Xms256m -Dfile.encoding=UTF-8 -Dlog.level.no.nav=WARN</argLine> <!-- default argLine for surefire-plugin -->
